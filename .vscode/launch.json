    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "envFile": "${workspaceFolder}/.env",
            "request": "launch",
            "name": "Build & Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\out\\tmq.js",
            "preLaunchTask": "npm: build",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "envFile": "${workspaceFolder}/.env",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\out\\tmq.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}